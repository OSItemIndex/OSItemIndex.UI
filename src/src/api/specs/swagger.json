{
  "swagger": "2.0",
  "info": {
    "title": "OSItemIndex.API",
    "version": "v1"
  },
  "paths": {
    "/items": {
      "get": {
        "tags": [
          "Items"
        ],
        "summary": "Returns a array of all items in the current database, with optional query filtering",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "query",
            "name": "Name",
            "type": "string"
          },
          {
            "in": "query",
            "name": "LooseCompare",
            "type": "boolean"
          },
          {
            "in": "query",
            "name": "Duplicate",
            "type": "boolean"
          },
          {
            "in": "query",
            "name": "Noted",
            "type": "boolean"
          },
          {
            "in": "query",
            "name": "Placeholder",
            "type": "boolean"
          },
          {
            "in": "query",
            "name": "Stackable",
            "type": "boolean"
          },
          {
            "in": "query",
            "name": "TradeableOnGe",
            "type": "boolean"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/OsrsBoxItem"
              }
            }
          }
        }
      }
    },
    "/items/{id}": {
      "get": {
        "tags": [
          "Items"
        ],
        "summary": "Returns an item matching the specified Item ID",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "description": "Item ID",
            "required": true,
            "type": "integer",
            "format": "int32"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/OsrsBoxItem"
            }
          }
        }
      }
    },
    "/version": {
      "get": {
        "tags": [
          "Items"
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "definitions": {
    "OsrsBoxItem": {
      "type": "object",
      "properties": {
        "id": {
          "format": "int32",
          "description": "Unique OSRS item ID number.",
          "type": "integer"
        },
        "name": {
          "description": "The name of the item.",
          "type": "string"
        },
        "duplicate": {
          "description": "If the item is a duplicate.",
          "type": "boolean"
        },
        "noted": {
          "description": "If the item is noted.",
          "type": "boolean"
        },
        "placeholder": {
          "description": "If the item is a placeholder.",
          "type": "boolean"
        },
        "stackable": {
          "description": "If the item is stackable (in inventory).",
          "type": "boolean"
        },
        "tradeable_on_ge": {
          "description": "If the item is tradeable (only on GE).",
          "type": "boolean"
        },
        "last_updated": {
          "format": "date-time",
          "description": "The last time (UTC) the item was updated (in ISO8601 date format).",
          "type": "string"
        },
        "document": {
          "description": "OSRSBox item document.",
          "type": "string"
        }
      }
    }
  }
}